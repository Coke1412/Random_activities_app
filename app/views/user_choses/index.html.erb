<p id="notice"><%= notice %></p>

<h1>User Choses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_choses.each do |user_chose| %>
      <tr>
        <td><%= link_to 'Show', user_chose %></td>
        <td><%= link_to 'Edit', edit_user_chose_path(user_chose) %></td>
        <td><%= link_to 'Destroy', user_chose, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

